{
  "name": "@bync/api",
  "version": "0.0.1",
  "description": "",
  "author": "Matheus Poleza",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate-types": "npx supabase gen types typescript --project-id atbffzwecdubmzwfinqr > ./src/infrastructure/database/__generated__/database.types.ts",
    "db:pull": "npx supabase link --project-ref atbffzwecdubmzwfinqr && npx supabase db pull",
    "db:storage:pull": "npx supabase link --project-ref atbffzwecdubmzwfinqr && npx supabase db pull --schema storage",
    "db:migrate": "npx supbase migration up",
    "db:reset": "npx supabase db reset",
    "db:deploy": "npx supabase db push",
    "db:migration:create": "npx supabase migration new"
  },
  "dependencies": {
    "@js-joda/core": "^5.6.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "@supabase/supabase-js": "^2.39.3",
    "@upstash/redis": "^1.28.2",
    "archiver": "^6.0.1",
    "axios": "^1.6.7",
    "belvo": "^0.28.0",
    "fast-csv": "^4.3.6",
    "form-data": "^4.0.0",
    "formdata-node": "^6.0.3",
    "mysql2": "^3.6.5",
    "node-fetch": "^3.3.2",
    "puppeteer": "^21.5.2",
    "puppeteer-core": "^21.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stream-buffers": "^3.0.2",
    "supabase": "1.136.2",
    "type-fest": "^4.10.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.1",
    "ynab": "^2.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/belvo": "^0.16.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "2.x",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
