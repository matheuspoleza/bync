{
  "name": "@bync/ui",
  "version": "2.156.0",
  "exports": {
    ".": {
      "types": "./build/cjs/main.d.ts",
      "import": "./build/esm/main.mjs",
      "default": "./build/cjs/main.cjs"
    },
    "./global.css": {
      "types": "./build/cjs/global.css.d.ts",
      "import": "./build/esm/global.css.vanilla.mjs",
      "default": "./build/cjs/global.css.vanilla.cjs"
    },
    "./theme.css": {
      "types": "./build/cjs/theme.css.d.ts",
      "import": "./build/esm/theme.css.vanilla.mjs",
      "default": "./build/cjs/theme.css.vanilla.cjs"
    },
    "./styles": {
      "types": "./build/cjs/styles/index.d.ts",
      "import": "./build/esm/styles/index.mjs",
      "default": "./build/cjs/styles/index.cjs"
    },
    "./utils": {
      "types": "./build/cjs/utils/index.d.ts",
      "import": "./build/esm/utils/index.mjs",
      "default": "./build/cjs/utils/index.cjs"
    },
    "./next": {
      "types": "./build/next/main.d.ts",
      "default": "./build/next/main.js"
    },
    "./next/styles": {
      "types": "./build/next/styles/index.d.ts",
      "default": "./build/next/styles/index.js"
    },
    "./next/utils": {
      "types": "./build/next/utils/index.d.ts",
      "default": "./build/next/utils/index.js"
    },
    "./next/stories": {
      "types": "./build/next/stories.d.ts",
      "default": "./build/next/stories.js"
    },
    "./next/type-docs": {
      "types": "./build/next/type-docs.d.ts",
      "default": "./build/next/type-docs.js"
    },
    "./next/global.css": {
      "types": "./build/next/global.css.d.ts",
      "default": "./build/next/global.css.js"
    },
    "./next/theme.css": {
      "types": "./build/next/theme.css.d.ts",
      "default": "./build/next/theme.css.js"
    }
  },
  "main": "build/cjs/main.cjs",
  "types": "build/cjs/main.d.ts",
  "typesVersions": {
    "*": {
      "global.css": [
        "./build/cjs/global.css.d.ts"
      ],
      "theme.css": [
        "./build/cjs/theme.css.d.ts"
      ],
      "styles": [
        "./build/cjs/styles/index.d.ts"
      ],
      "utils": [
        "./build/cjs/utils/index.d.ts"
      ],
      "next": [
        "./build/next/main.d.ts"
      ],
      "next/stories": [
        "./build/next/stories.d.ts"
      ],
      "next/type-docs": [
        "./build/next/type-docs.d.ts"
      ],
      "next/styles": [
        "./build/next/styles/index.d.ts"
      ],
      "next/utils": [
        "./build/next/utils/index.d.ts"
      ],
      "next/global.css": [
        "./build/next/global.css.d.ts"
      ],
      "next/theme.css": [
        "./build/next/theme.css.d.ts"
      ]
    }
  },
  "scripts": {
    "prebuild": "yarn g:rimraf build",
    "build": "yarn prebuild && yarn build:typedocs && yarn g:run-p build:lib build:next && yarn postbuild",
    "postbuild": "yarn g:run-p postbuild:lib postbuild:next",
    "build:lib": "rollup -c",
    "postbuild:lib": "yarn g:run-p postbuild:lib:cjs postbuild:lib:esm",
    "build:lib:declaration:cjs": "tsc --project tsconfig.build.json --outDir build/cjs --module commonjs --emitDeclarationOnly true && yarn g:tsc-alias -p tsconfig.build.json --outDir build/cjs",
    "build:lib:declaration:esm": "tsc --project tsconfig.build.json --outDir build/esm --module esnext --emitDeclarationOnly true && yarn g:tsc-alias -p tsconfig.build.json --outDir build/esm",
    "build:lib:package:cjs": "touch build/cjs/package.json && echo '{ \"type\": \"commonjs\" }' > build/cjs/package.json",
    "build:lib:package:esm": "touch build/esm/package.json && echo '{ \"type\": \"module\" }' > build/esm/package.json",
    "build:next": "yarn g:tsc -p tsconfig.build.json --outDir build/next --module esnext && yarn g:tsc-alias -p tsconfig.build.json --outDir build/next",
    "postbuild:next": "yarn build:next:package",
    "build:next:package": "touch build/next/package.json && echo '{ \"type\": \"module\" }' > build/next/package.json",
    "build:storybook": "storybook build",
    "build:typedocs": "ts-node -T src/doc-generation/index.ts",
    "chromatic:publish": "IS_CHROMATIC=true yarn g:chromatic --project-token=chpt_8d4082a96c9204f --build-script-name=build:storybook",
    "coverage": "vitest --coverage",
    "dev": "yarn storybook",
    "lint": "yarn g:run-p lint:eslint \"lint:prettier --check\"",
    "lint:eslint": "yarn g:eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "yarn g:run-p \"lint:eslint --fix\" \"lint:prettier --write\"",
    "lint:prettier": "yarn g:prettier . \"!**/*.{js,ts,tsx}\"",
    "lint:report": "yarn g:eslint-output --quiet './src/**/*.{js,ts,tsx}'",
    "postbuild:lib:cjs": "yarn g:run-p build:lib:declaration:cjs build:lib:package:cjs",
    "postbuild:lib:esm": "yarn g:run-p build:lib:declaration:esm build:lib:package:esm",
    "storybook": "storybook dev -p 6006",
    "test": "yarn g:vitest",
    "test:check": "yarn g:tsc --noEmit",
    "test:dependencies": "yarn g:depcheck",
    "test:unit": "yarn g:vitest run"
  },
  "prettier": "@bync-meta/prettier-config",
  "eslintConfig": {
    "extends": "@bync-meta/eslint-config"
  },
  "dependencies": {
    "@bync/icons": "workspace:*",
    "@codemirror/autocomplete": "6.9.1",
    "@codemirror/commands": "6.3.0",
    "@codemirror/lang-javascript": "6.2.1",
    "@codemirror/lang-json": "6.0.1",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@lezer/highlight": "1.1.6",
    "@popperjs/core": "2.11.7",
    "@seznam/compose-react-refs": "1.0.6",
    "@tanstack/react-virtual": "3.0.0-beta.54",
    "@uiw/codemirror-themes": "4.21.18",
    "@uiw/react-codemirror": "4.21.18",
    "codemirror": "6.0.1",
    "codemirror-readonly-ranges": "^0.1.0-alpha.2",
    "dayjs": "1.11.9",
    "lodash": "4.17.21",
    "polished": "4.2.2",
    "prismjs": "1.29.0",
    "react-collapsed": "4.0.2",
    "react-dismissable-layers": "0.3.2",
    "react-dropzone": "14.2.3",
    "react-popper": "2.3.0",
    "react-slider": "2.0.4",
    "react-textarea-autosize": "8.5.0",
    "react-toastify": "9.1.3",
    "slate": "0.94.1",
    "slate-history": "0.93.0",
    "slate-react": "0.98.1",
    "ts-pattern": "5.0.5",
    "type-fest": "3.13.1"
  },
  "devDependencies": {
    "@bync-meta/eslint-config": "workspace:*",
    "@bync-meta/prettier-config": "workspace:*",
    "@bync-meta/storybook-config": "workspace:*",
    "@bync-meta/typescript-config": "workspace:*",
    "@bync-meta/vitest-config": "workspace:*",
    "@bync/style": "workspace:*",
    "@faker-js/faker": "8.0.2",
    "@testing-library/react-hooks": "8.0.1",
    "@types/prismjs": "1.26.0",
    "@types/react": "18.0.35",
    "@types/react-slider": "1.3.1",
    "@types/react-textarea-autosize": "4.3.6",
    "@vanilla-extract/rollup-plugin": "1.3.0",
    "csstype": "3.1.2",
    "jotai": "2.3.1",
    "jotai-molecules": "1.1.1",
    "react-dom": "18.2.0",
    "resize-observer-polyfill": "1.5.1",
    "rollup": "3.26.0",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "rollup-plugin-tsconfig-paths": "1.5.1",
    "storybook": "7.2.1",
    "ts-node": "10.9.1"
  },
  "peerDependencies": {
    "@bync/style": "workspace:*",
    "jotai": "^2",
    "jotai-molecules": "^1",
    "react": "^18",
    "react-dom": "^18"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "tag": "next"
  }
}
